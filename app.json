{
  "expo": {
    "name": "MyCarLog",
    "slug": "MyCarLog",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f8f7f8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MyCarLog"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#f8f7f8"
      },
      "package": "com.anonymous.MyCarLog"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "70e0240a-4ed5-4b6f-9c8f-386d94f070a7"
      }
    }
  }
}
